<div class="tree-level">
  <h3>Agent <%= agent_run.name %></h3>
  <ul class="pl-2">
    <% agent_run.events.each do |event| %>
      <li>
      <% if event.type == 'run_agent' %>
        <%= render partial: 'agent_runs/tree_view', locals: { agent_run: event.new_agent } %>
      <% else %>
        <%= render partial: 'agent_runs/event', locals: { event: event } %>
      <% end %>
      </li>
    <% end %>
  </ul>
</div>